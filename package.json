{
  "name": "find-main",
  "main": "./release",
  "version": "2.0.0",
  "description": "Find the \"main\" path from package.json",
  "license": "MIT",
  "repository": "bendrucker/find-main",
  "author": {
    "name": "Ben Drucker",
    "email": "bvdrucker@gmail.com",
    "url": "bendrucker.me"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "standard && babel-tape-runner test/*.js"
  },
  "keywords": [
    "main",
    "package.json",
    "find"
  ],
  "dependencies": {
    "mothership": "~0.3.0"
  },
  "devDependencies": {
    "babel": "^5.0.0",
    "babel-tape-runner": "*",
    "proxyquire": "~1.5.0",
    "publicist": "~4.0.0",
    "publicist-es5": "~1.0.0",
    "standard": "bendrucker/standard#babel",
    "tape": "^4.0.0"
  },
  "publicist": {
    "builds": {
      "es5": {
        "dest": "./release"
      }
    }
  },
  "standard": {
    "ignore": [
      "release/",
      "lib/"
    ]
  }
}
